{
  "name": "onlinejudge",
  "version": "0.2.2",
  "main": "serevr.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "mock": "./node_modules/.bin/ts-node ./src/task/mock.ts",
    "test": "nyc mocha --compilers js:babel-core/register ./test/index.js --timeout=60000",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "callsite": "^1.0.0",
    "conenv": "^0.1.4",
    "config": "^1.26.1",
    "crypto-js": "^3.1.9-1",
    "glob": "^7.1.1",
    "ioredis": "^2.5.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-jwt": "^3.2.2",
    "koa-logger": "^3.0.0",
    "koa-router": "^7.1.1",
    "mysql2": "^1.3.2",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.0-alpha.15",
    "sequelize": "^4.0.0-1",
    "sequelize-typescript": "^0.3.2",
    "typedi": "^0.5.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.32",
    "@types/crypto-js": "^3.1.33",
    "@types/ioredis": "^0.0.23",
    "@types/jsonwebtoken": "^7.2.1",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.23",
    "@types/koa-jwt": "^3.2.0",
    "@types/koa-logger": "^2.0.2",
    "@types/node": "7.0.28",
    "@types/sequelize": "4.0.55",
    "@types/supertest": "^2.0.0",
    "chai": "^3.5.0",
    "codecov": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.3.0",
    "mocha-typescript": "^1.1.4",
    "mockjs": "^1.0.1-beta3",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.2",
    "supertest": "^3.0.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.4.2",
    "tslint-config-standard": "^5.0.2",
    "typescript": "^2.3.4"
  },
  "nyc": {
    "all": true,
    "cache": true,
    "exclude": [
      "coverage",
      "task",
      "test"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
