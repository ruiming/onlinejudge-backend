{
  "name": "onlinejudge",
  "version": "0.2.2",
  "main": "serevr.js",
  "license": "MIT",
  "scripts": {
    "test": "nyc mocha --compilers js:babel-core/register ./test/index.js --timeout=60000",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "mock": "mock=true mocha --compilers js:babel-core/register ./task/mock.js --timeout=60000"
  },
  "dependencies": {
    "conenv": "^0.1.4",
    "crypto-js": "^3.1.9-1",
    "glob": "^7.1.1",
    "ioredis": "^2.5.0",
    "leibniz": "https://github.com/ruiming/leibniz.git",
    "require-dir": "^0.3.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codecov": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.3.0",
    "nyc": "^10.3.2",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "all": true,
    "cache": true,
    "exclude": [
      "coverage",
      "task",
      "test"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
